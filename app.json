{
  "expo": {
    "name": "MobTimon",
    "slug": "mob",
    "version": "2.0",
    "orientation": "portrait",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "icon": "./src/images/modelo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/images/LogoMobSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mobtimon.teste"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/images/modelo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.lsm_00.mobtimon",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"

      ]
    },
    "web": {
      "favicon": "../src/images/modelo.png"
    },
    "extra": {
      "eas": {
        "projectId": "c97f74f5-04ff-4412-844a-cab1750c8309"
      }
    }
  }
}
